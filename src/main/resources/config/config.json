{
	"debugMode": false,
	"compiler": {
		"classGenerationDirectory": "${java-temp}/boxlang/classes"
	},
	"runtime": {
		"mappings": {
			"/": "${user-dir}"
		},
		"modulesDirectory": [
			"${user-home}/.boxlang/modules"
		],
		"customTagsDirectory": [
			"${user-home}/.boxlang/customTags"
		],
		"caches": {
			"imports": {
				"type": "Caffeine",
				"properties": {
					"maximumSize": 1000
				}
			},
			"default": {
				"type": "Caffeine",
				"properties": {
					"maximumSize": 1000
				}
			}
		}
	}
}